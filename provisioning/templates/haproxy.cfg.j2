# Do not edit, changes will be overwritten

global
    log /dev/log local0
    log /dev/log local0 notice
    user haproxy
    group haproxy
    daemon

defaults
    mode tcp

frontend vault-fe
    bind :443
    default_backend vault-api

backend vault-api
{% for host in groups['vault'] %}
    server srv-{{ loop.index }} {{ host }}:8200 check
{% endfor %}